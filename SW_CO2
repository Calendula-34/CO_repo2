#include<stdio.h>

void swap(int *pa, int *pb)
{
	int temp = 0
	temp = *pa;
	*pa = *pb;
	*pb = temp;
	//printf("swap에서 변경 후 : a = %d, b = %d\n", a, b);
}

void main()
{
	int a = 10;
	int b = 20;

	printf("변경 전 : a = %d, b = %d\n", a, b);
	swap(&a, &b);
	printf("main에서 변경 후 : a = %d, b = %d\n", a, b);
}
